{
  "manifest_version": 2,
  "name": "ApplicationHub",
  "description": "A playground for my project",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "clipboardRead",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}